        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sub6_hours" />

    <TextView
        android:id="@+id/sub1_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="SUBJECT NAME"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/sub1_hours"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/sub1_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:inputType="text"
        android:text="SUBJECT EDIT"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/sub1_hours"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sub2_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="SUBJECT NAME"
        android:textSize="14sp"
        app:layout_constraintStart_toEndOf="@+id/sub2_hours"
        app:layout_constraintTop_toBottomOf="@+id/sub1_name" />

    <EditText
        android:id="@+id/sub2_edit"
